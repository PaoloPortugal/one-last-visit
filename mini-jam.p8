-- bY: "MOOFYS", "eLHOMBRELLAVE", "mAKU"

-->8
-- main

#include gameplay.lua
#include visuals.lua
#include entities.lua

function _init()
-- runs once at the start
    s_x=29*8 -- spawn x position
    s_y=38*8 -- spawn y position
    player=make_player(s_x,s_y)
    local chair=make_chair(8*8,30*8)
    cam=make_camera(player)
    messages={}
    objects={chair}
    timeshift=false
end

function _update60()
-- runs at 60fps
    player:update(objects)
    timeshift=player.y>60*8
    for obj in all(objects,dialogue.active) do
        obj:update()
    end
    cam:update()
end

function _draw()
-- runs at 60fps
    cls(0)

    camera(cam:cam_pos())

    draw_base_map(timeshift)

    for obj in all(objects) do
        obj:draw()
    end

    player:draw()

    draw_map_over()

    camera(0,0)

    player:draw_hud()
    
end

