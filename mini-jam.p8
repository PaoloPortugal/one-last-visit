-- main

#include gameplay.lua
#include visuals.lua
#include entities.lua

function _init()
-- runs once at the start
    s_x=2*8 -- spawn x position
    s_y=2*8 -- spawn y position
    player=make_player(s_x,s_y)
    cam=make_camera(player)
    messages={}
end

function _update60()
-- runs at 60fps
    player:update()
    cam:update()
end

function _draw()
-- runs at 60fps
    cls(0)

    camera(cam:cam_pos())

    map()

    player:draw()
    
end

