-- mINI jAM gAME
-- bY: "MOOFYS", "eLHOMBRELLAVE", "mAKU"

-->8
-- main

#include gameplay.lua
#include visuals.lua
#include entities.lua

function _init()
-- runs once at the start
    spawn_player()
    spawn_objects()
    messages={}
    make_pantry()
    spawn_dialogue(player.x,player.y-16,{"So this old place finally got sold, huh","I used to spend lots of time here with grandma... Good times...", "I wanna have a look at the backyard, it was always so peaceful there", "Huh, isn't the time on that clock wrong?"})
end

function _update60()
-- runs at 60fps
    player:update(objects)
    update_objects()
    cam:update()
    update_dialogue()
end

function _draw()
-- runs at 60fps
    cls(0)

    camera(cam:cam_pos())

    for c in all(clocks) do
        c:draw_fade()
    end

    draw_base_map(player.timeshift)

    for obj in all(objects) do
        obj:draw()
    end

    player:draw()

    draw_map_over(player.timeshift)

    draw_dialogue()

    camera(0,0)

    player:draw_hud()
    
end

function spawn_player()
    s_x=29*8 -- spawn x position
    s_y=32*8 -- spawn y position
    player=make_player(s_x,s_y)
    cam=make_camera(player)
end

function spawn_objects()
    local chair=make_chair(8*8,30*8)
    local clock_1,clock_1_m=make_clock(20*8,25*8)
    local safe=make_safe(20*8,30*8)
    clocks={clock_1,clock_1_m}
    objects={chair,clock_1,clock_1_m,safe}
end

function update_objects()
    for obj in all(objects) do
        obj:update()
    end
end

